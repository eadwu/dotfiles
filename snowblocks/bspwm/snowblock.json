[
  {
    "clean": [
      "~/.bspwm"
    ]
  },
  {
    "link": {
      "~/.bspwm/bin/kbd_backlight": {
        "create": true,
        "force": true
      },
      "~/.bspwm/bin/mon_backlight": {
        "force": true
      },
      "~/.bspwm/bspwmrc": {
        "force": true
      },
      "~/.bspwm/cc": {
        "force": true
      },
      "~/.bspwm/config.ini": {
        "force": true
