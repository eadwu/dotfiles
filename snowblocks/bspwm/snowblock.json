[
  {
    "clean": [
      "~/.bspwm"
    ]
  },
  {
    "link": {
      "~/.bspwm": {
        "create": true,
        "force": true,
        "path": "."
      }
    }
  },
  {
    "shell": [
      [
        "sudo chmod +x ~/.bspwm/{bspwmrc,external_rules,java_nonreparenting_wm_hack,bin/kbd_backlight,bin/mon_backlight}",
        "Adjust file permissions"
      ]
    ]
  }
]
