      "~/.bspwm/java_nonreparenting_wm_hack": {
        "force": true
      },
      "~/.bspwm/sxhkdrc": {
        "force": true
      },
      "~/.bspwm/weather_icons": {
        "force": true
      }
    }
  }
]
